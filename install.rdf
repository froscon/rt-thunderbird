<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>RTPlugin@FrOSCon</em:id>
    <em:version>0.1.1</em:version>

    <!-- Target Application this extension can install into,
        with minimum and maximum supported versions. -->
    <em:targetApplication>
      <Description>
        <!-- Thunderbird -->
        <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
        <em:minVersion>24.0</em:minVersion>
        <em:maxVersion>48.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Front End MetaData -->
    <em:name>RT by FrOSCon</em:name>
    <em:description>Delete Spam and Take tickets in RT.</em:description>
    <em:creator>Martin Lang &amp; Gereon Kremer</em:creator>
    <em:optionsURL>chrome://frosconrt/content/preferences.xul</em:optionsURL>
  </Description>
</RDF>
